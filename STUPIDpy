def add_commas(input_string):
    # Remove leading and trailing square brackets and extra spaces
    cleaned_string = input_string.strip()[1:-1]  # remove leading '[' and trailing ']'
    
    # Replace multiple spaces with a single space
    cleaned_string = ' '.join(cleaned_string.split())
    print(cleaned_string)
    # Split the cleaned string by spaces to get individual numbers
    numbers = cleaned_string.split()
    
    # Join the numbers with commas
    result = ', '.join(numbers)
    
    return result

